{
  "manifest_version": 2,
  "name": "stayForever",
  "version": "2.0.0",
  "icons" : {
  "16": "logo.png",
  "48": "logo.png",
  "128": "logo.png"
  },
  
  "browser_action" : {
    "default_icon": "logo.png"
  },
  
  
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["Content.js"]
    }
  ]
}
